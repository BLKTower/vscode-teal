{
    "name": "vscode-tl",
    "displayName": "vscode-tl",
    "description": "TL language support for Visual Studio Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "tl",
            "aliases": ["TL", "tl"],
            "extensions": [".tl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tl",
            "scopeName": "source.tl",
            "path": "./syntaxes/tl.tmLanguage.json"
        }]
    }
}